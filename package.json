{
  "name": "ChordFiddle",
  "version": "1.2.1",
  "description": "ChordPro editor. Transpose your chord sheet. Convert ChordPro to chord sheet.",
  "main": "index.html",
  "scripts": {
    "start": "react-scripts-x start",
    "build": "react-scripts-x build",
    "test": "react-scripts-x test --env=jsdom",
    "eject": "react-scripts-x eject"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/martijnversluis/chordfiddle.git"
  },
  "keywords": [
    "javascript",
    "chords",
    "parsing",
    "convert",
    "chord-sheet",
    "chordpro"
  ],
  "author": "martijnversluis@gmail.com",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/martijnversluis/chordfiddle/issues"
  },
  "homepage": "https://github.com/martijnversluis/chordfiddle",
  "about": "https://github.com/martijnversluis/ChordFiddle/blob/master/README.md#chordfiddle",
  "dependencies": {
    "chordjs": "^1.2.0",
    "chordsheetjs": "^1.5.3",
    "postcss-calc": "^6.0.1",
    "postcss-custom-properties": "^7.0.0",
    "postcss-import": "^11.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.0.17",
    "react-scripts-x": "^0.1.3"
  },
  "devDependencies": {
    "node-sass": "^4.7.2"
  },
  "react-scripts-x": {
    "postcss": [
      {
        "name": "postcss-import"
      },
      {
        "name": "postcss-custom-properties",
        "config": {
          "preserve": true
        }
      },
      {
        "name": "postcss-calc"
      }
    ]
  }
}
