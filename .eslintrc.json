{
  "parser": "babel-eslint",

  "parserOptions": {
    "sourceType": "module"
  },

  "env": {
    "amd": true,
    "browser": true,
    "es6": true,
    "jest": true
  },

  "extends": "airbnb",

  "rules": {
    "max-len": [2, 120, 2],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
